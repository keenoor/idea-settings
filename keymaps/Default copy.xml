<keymap version="1" name="Default copy" parent="$default">
  <action id="ECTranslation">
    <keyboard-shortcut first-keystroke="meta i" />
    <keyboard-shortcut first-keystroke="alt i" />
  </action>
  <action id="Move">
    <keyboard-shortcut first-keystroke="shift f6" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="f6" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="ViewSource">
    <keyboard-shortcut first-keystroke="f4" />
  </action>
</keymap>